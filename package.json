{
    "name":  "newmcp",
    "version":  "1.0.0",
    "description":  "Selenium test framework with Chrome DevTools MCP integration",
    "main":  "index.js",
    "scripts":  {
                    "test":  "mocha --require ts-node/register \u0027src/tests/**/*.spec.ts\u0027",
                    "test:watch":  "mocha --require ts-node/register --watch \u0027src/tests/**/*.spec.ts\u0027",
                    "capture":  "ts-node scripts/capture-flow.ts",
                    "build":  "tsc"
                },
    "keywords":  [
                     "selenium",
                     "testing",
                     "typescript",
                     "mocha"
                 ],
    "author":  "",
    "license":  "ISC",
    "dependencies":  {
                         "chai":  "^4.3.10",
                         "selenium-webdriver":  "^4.15.0"
                     },
    "devDependencies":  {
                            "@types/chai":  "^4.3.11",
                            "@types/mocha":  "^10.0.6",
                            "@types/node":  "^20.10.0",
                            "@types/selenium-webdriver":  "^4.35.4",
                            "mocha":  "^10.2.0",
                            "ts-node":  "^10.9.2",
                            "typescript":  "^5.3.3"
                        }
}
